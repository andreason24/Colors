<!doctype HTML>
<html>
<head>
  <title>Canvassing</title>
  <style>
    body {
      font-family:"helvetica neue", arial, sans-serif ;
      margin:0;
      padding:0;
    }


    #whisky {
      padding:0;
      width: 500px;
      height:500px;

    }
  </style>
</head>
<body>
  <h1 class="text-center">Canvas practice</h1>
  <div class="row">
    <div class="col-md-6">
      <div class="col-md-offset-3">
        <canvas id="whisky" width="500" height="500"> </canvas>
      </div>
    </div>

  </div>
  <script>
    var emitDraw = emit('whisky');
    emitDraw.start();
  </script>
</body>
</html>